<h1><%= t('.header') %></h1>
 <%= render 'shared/errors', resource: @test %>
<table class="table table-bordered">
  <tr>
    <th class="sort-by-title pointer">
      <%= t('.title') %>
      <%= octicon 'arrow-up', class: 'text-success ' %>
      <%= octicon 'arrow-down', class: 'text-success ' %>
    </th>
    <th><%= t('.level') %></th>
    <th><%= t('.questions')%></th>
    <th colspan="4"><%= t('.actions')%></th>
  </tr>
  <%= render @tests %>
</table>
<%= link_to t('.create_test_btn'), new_admin_test_path, class: "btn btn-primary" %>
