<h1>Single test</h1>
<h1>Type: <%=	@test.title %></h1>
<h1>Level: <%=	@test.level %></h1>
<p>List of questions</p>
<table>
  <tr>
    <th>Title</th>
    <th colspan="3">Action</th>
  </tr>

  <%= render partial: 'questions/question', collection: @test.questions %>

</table>
<%= link_to 'Edit', edit_admin_test_path(@test) %> |
<%= link_to 'New question', new_test_question_path(@test.id) %> |
<%= link_to 'Back', admin_tests_path %>