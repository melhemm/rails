<%= form_with model: [@user, @feedback] do |form| %>
  <%= render 'shared/errors', resource: @feedback%>
    <div class="form-group">
      <%= form.label :message %><br>
      <%= form.text_field :message, id: :message, class: "form-control" %>
    </div>
    <%= form.submit "Send message", class: "btn btn-success mt-2 mb-2" %>
<% end %>
